<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="edu.utsa.cs3443.questlog.ui.CreditsController"
      spacing="8">

    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>

    <HBox spacing="10">
        <Button text="Back" onAction="#onBackClicked"/>
        <Label text="Credits" styleClass="screen-title"/>
    </HBox>

    <Separator/>

    <Label text="Designers: ..."/>
    <Label text="Developers: ..."/>
    <Label text="Project Manager: ..."/>
    <Label text="Tester: ..."/>

</VBox>
